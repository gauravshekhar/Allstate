#footer-content
	margin 0 auto
	width 1001px
	background-color #C0C0C0
	margin-bottom 10px
	border 1px solid #C0C0C0

	ul
		font-size 13px
		display block
		float right
		padding 5px 10px 5px 0

		li
			display inline
			margin 0 8px 0 0

			a
				color #FFF

			a:hover
				text-decoration underline